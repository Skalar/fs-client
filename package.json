{
  "name": "@westerdals/fs-client",
  "version": "0.0.8",
  "description": "Felles Studentsystem (FS) API client",
  "main": "lib/fs.js",
  "scripts": {
    "compile": "rm -fr lib && babel -d lib/ src/",
    "prepublish": "npm run compile",
    "lint": "eslint src test",
    "test": "NODE_ENV=test babel-node node_modules/.bin/tape \"./test/**/*.test.js\"",
    "ci-test": "npm run lint && npm test",
    "watch": "nodemon -q -x 'npm test'"
  },
  "bin": {
    "fs-client": "bin/fs-client.js"
  },
  "engines": {
    "node": "5.3.0"
  },
  "author": "Gudleik Rasch <gr@skalar.no>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "3.1.1",
    "request": "2.67.0",
    "xml2js": "0.4.15"
  },
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.3.13",
    "blue-tape": "^0.1.11",
    "eslint": "1.10.3",
    "nock": "^5.2.1",
    "tape": "^4.4.0"
  }
}
